@prefix ns1: <https://qudt.org/schema/qudt/> .
@prefix ns2: <https://github.com/DeperiasKerre/qcl_Onto/blob/main/qclontology/version-1.0/qclonto.owl#> .
@prefix ns3: <https://dcmi.github.io/bibo/#:> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns2:BoundToContinum a ns2:LaserDesignType .

ns2:ContinousWaveOperation a ns2:LaserWorkingMode .

ns2:DOI rdfs:range xsd:string .

ns2:DoubleResonantPhonon a ns2:LaserDesignType .

ns2:LOPhononDepopulation a ns2:LaserDesignType .

ns2:LasingFrequency a <https://w3id.org/mdo/core/Property> ;
    prov:wasAttributedTo <http://purl.org/ontology/bibo/AcademicArticle> ;
    ns2:relatesToHeterostructure ns2:LaserHeterostructure ;
    ns1:hasUnit <https://qudt.org/vocab/unit/TeraHZ> .

ns2:OpticalPower a <https://w3id.org/mdo/core/Property> ;
    prov:wasAttributedTo <http://purl.org/ontology/bibo/AcademicArticle> ;
    ns2:relatesToHeterostructure ns2:LaserHeterostructure ;
    ns1:hasUnit <https://qudt.org/vocab/unit/MilliW> .

ns2:PulsedOperation a ns2:LaserWorkingMode .

ns2:URL rdfs:range schema:URL .

ns2:WorkingTemperature a <https://w3id.org/mdo/core/Property> ;
    rdfs:label "Working Temperature" ;
    prov:wasAttributedTo <http://purl.org/ontology/bibo/AcademicArticle> ;
    ns2:correspondsToWorkingMode ns2:LaserWorkingMode ;
    ns2:relatesToHeterostructure ns2:LaserHeterostructure ;
    ns1:hasUnit <https://qudt.org/vocab/unit/K> .

ns2:matFormula rdfs:range xsd:string .

ns1:numericValue rdfs:range schema:Number .

ns2:LaserHeterostructure prov:wasAttributedTo <http://purl.org/ontology/bibo/AcademicArticle> ;
    ns2:hasDesignType ns2:LaserDesignType ;
    ns2:hasMaterials ns2:HeterostructureMaterials .

ns2:LaserWorkingMode prov:wasAttributedTo <http://purl.org/ontology/bibo/AcademicArticle> .

<http://purl.org/ontology/bibo/AcademicArticle> rdfs:subClassOf <https://www.w3.org/TR/prov-o/#Agent> ;
    ns3:cites <http://purl.org/ontology/bibo/AcademicArticle> .

